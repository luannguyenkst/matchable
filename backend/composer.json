{
    "name": "matchable/checkout-api",
    "description": "Matchable Checkout System API",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.5",
        "firebase/php-jwt": "^6.8",
        "stripe/stripe-php": "^10.15",
        "phpmailer/phpmailer": "^6.8",
        "predis/predis": "^2.2",
        "ramsey/uuid": "^4.7",
        "respect/validation": "^2.2",
        "monolog/monolog": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs-fix": "php-cs-fixer fix src/",
        "analyze": "phpstan analyse src/"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}